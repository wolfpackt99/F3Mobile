@model IEnumerable<F3.ViewModels.News>
@{
    ViewBag.Title = "Home Page";
}
<div class="row">
    <div class="col-sm-12 col-md-8">
        <div class="row">
            <div class="well-sm">
                <p>@Html.ActionLink("FNG Signup", MVC.Newsletter.Actions.Index(), new { @class = "btn btn-primary btn-sm" }, "https")</p>
            </div>
        </div>
        <div class="row body-content">
            <div>
                <h4>News</h4> 

                <ul class="list-group">
                    @Html.DisplayFor(m => Model)
                </ul>

            </div>

        </div>
    </div>
    <div class="col-sm-12 col-md-4">
        <a class="twitter-timeline" href="https://twitter.com/trentjones/lists/f3-accounts" data-height="500" data-chrome="noheader nofooter">F3 South Charlotte</a>
    </div>
</div>

@section scripts{
    <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
}

